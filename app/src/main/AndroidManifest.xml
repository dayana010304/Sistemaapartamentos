<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sistema.apartamentos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icono"
        android:supportsRtl="true"
        android:theme="@style/Theme.SistemaApartamentos">
        <activity android:name=".perfiluserActivity"></activity>
        <activity android:name=".EditAparmentActivity" />
        <activity android:name=".ListApartmentActivity" />
        <activity android:name=".RegisterAparmentActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>